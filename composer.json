{
    "name": "zendframework/zend-stratigility-dispatch",
    "description": "Zend Stratigility Dispatch proposal for PHP middleware application based on PSR-7 standard",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "http",
        "psr-7",
        "middleware",
        "route",
        "dispatch"
    ],
    "require": {
        "php": ">=5.5",
        "psr/http-message": "~1.0",
        "container-interop/container-interop": "~1.0",
        "aura/router" : "~2.2"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.6",
        "zendframework/zend-diactoros": "~1.0",
        "squizlabs/php_codesniffer": "^2.3.1",
        "nikic/fast-route" : "~0.6"
    },
    "suggest" : {
        "nikic/fast-route" : "If you want to use the FastRoute route adapter"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Stratigility\\Dispatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Stratigility\\Dispatch\\": "test/"
        }
    }
}
